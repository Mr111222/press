/*eslint-disable*/
<%for(key in el[0]){%>
import <%=el[0][key]%> from 'element-ui/packages/<%=el[1][key]%>/index.js'
<%}%>
<%for(key in cb[0]){%>
import <%=cb[0][key]%> from './<%=cb[1][key]%>/src/main.js'
<%}%>
<%for(key in elPlugins[0]){%>
import <%=elPlugins[0][key]%> from 'element-ui/packages/<%=elPlugins[1][key]%>/index.js'
<%}%>
<%for(key in cbPlugins[0]){%>
import <%=cbPlugins[0][key]%> from './<%=cbPlugins[1][key]%>/src/main.js'
<%}%>
let elPlugins = {<%=elPlugins[0]%>}
let cbPlugins = {<%=cbPlugins[0]%>}
let cbComponents = {<%=cb[0]%>}
let elComponents = {<%=el[0]%>}

Object.keys(elComponents).forEach(key => {
  elComponents[key].install = function(Vue, options = {}) {
    if (elComponents[key].install.installed) return
    Vue.component(key, elComponents[key])
  }
})

function install (Vue, options = {}) {
  let components = {...elComponents, ...cbComponents}
  Object.keys(components).forEach(key => {
    Vue.use(components[key])
  })

  Vue.use(CbLoading.directive);
  Vue.prototype.$ELEMENT = {
    size: '',
    zIndex: 2000
  };

  Vue.prototype.$loading = CbLoading.service;
  Vue.prototype.$msgbox = CbMessageBox;
  Vue.prototype.$alert = CbMessageBox.alert;
  Vue.prototype.$confirm = CbMessageBox.confirm;
  Vue.prototype.$prompt = CbMessageBox.prompt;
  Vue.prototype.$notify = CbNotification;
  Vue.prototype.$message = CbMessage;

}

export default {install, ...elComponents, ...cbComponents, ...elPlugins, ...cbPlugins}
export {<%=elPlugins[0]%>}
export {<%=cbPlugins[0]%>}
export {<%=cb[0]%>}
export {<%=el[0]%>}